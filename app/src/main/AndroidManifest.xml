        <activity android:name=".SignificadoDeLosNumeros" />
        <activity android:name=".AcontecimientoDelDia" />
        <activity android:name=".TonicaDelDia" />
        <activity android:name=".TonicaFundamental" />
        <activity android:name=".UrgenciaInterior" />
        <activity android:name=".EstudioNumerologico" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>